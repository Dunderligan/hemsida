# postgres connection string
DATABASE_URL=postgresql://user:password@endpoint/dbname

# the domain where the auth API (and usually the frontend) is running
# used for OAuth redirect URIs
AUTH_URL=http://localhost:5173

# Blizzard Battle.net OAuth credentials
# https://community.developer.battle.net/access/clients
BATTLENET_CLIENT_ID=
BATTLENET_CLIENT_SECRET=

# S3-compatible storage (e.g. Cloudflare R2) credentials
# used for uploading team logos
S3_ENDPOINT=
S3_ACCESS_KEY_ID=
S3_SECRET_ACCESS_KEY=
S3_BUCKET_NAME=dunderligan

# where images and some static assets are served from
PUBLIC_CDN_ENDPOINT=https://cdn.dunderligan.se
